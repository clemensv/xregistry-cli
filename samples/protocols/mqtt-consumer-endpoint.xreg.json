{
  "endpoints": {
    "MyMqttConsumer": {
      "usage": [
        "consumer"
      ],
      "protocol": "MQTT/5.0",
      "protocoloptions": {
        "topic": "events",
        "qos": 1,
        "deployed": false,
        "endpoints": [
          {
            "uri": "mqtt://mybroker.example.com"
          }
        ]
      },
      "messagegroups": [
        "#/messagegroups/Contoso.MyGroup"
      ]
    }
  },
  "messagegroups": {
    "Contoso.MyGroup": {
      "protocol": "MQTT/5.0",
      "messages": {
        "Contoso.MyGroup.MyEvent": {
          "protocol": "MQTT/5.0",
          "protocoloptions": {
            "properties": {
              "topic": {
                "type": "uritemplate",
                "value": "clients/{clientid}/events/{eventid}"
              },
              "qos": {
                "type": "integer",
                "value": 1
              },
              "retain": {
                "type": "boolean",
                "value": false
              },
              "user-properties": [
                {
                  "name": "property1",
                  "type": "string"
                },
                {
                  "name": "property2",
                  "type": "integer"
                }
              ]
            }
          },
          "dataschemaformat": "JsonSchema/draft-07",
          "dataschemauri": "#/schemagroups/Contoso.MyEvents/schemas/MyEventData"
        }
      },
      "protocoloptions": {}
    }
  },
  "schemagroups": {
    "Contoso.MyEvents": {
      "schemas": {
        "MyEventData": {
          "format": "JsonSchema/draft-07",
          "versions": {
            "1.0": {
              "format": "JsonSchema/draft-07",
              "schema": {
                "$schema": "http://json-schema.org/draft-07/schema",
                "type": "object",
                "properties": {
                  "orderId": {
                    "type": "string"
                  },
                  "customerId": {
                    "type": "string"
                  },
                  "total": {
                    "type": "number"
                  },
                  "items": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "productId": {
                          "type": "string"
                        },
                        "quantity": {
                          "type": "number"
                        },
                        "price": {
                          "type": "number"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
