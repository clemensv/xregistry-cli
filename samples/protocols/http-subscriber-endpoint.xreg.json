{
  "$schema": "https://cloudevents.io/schemas/registry",
  "specversion": "0.5-wip",
  "endpoints": {
    "myhttpsubscriber": {
      "usage": [
        "subscriber"
      ],
      "protocol": "HTTP",
      "messagegroups": [
        "#/messagegroups/mygroup"
      ],
      "envelope": "CloudEvents/1.0",
      "protocoloptions": {
        "deployed": false,
        "endpoints": [
          {
            "uri": "https://cediscoveryinterop.azurewebsites.net/registry/subscriptions"
          }
        ]
      }
    }
  },
  "messagegroups": {
    "mygroup": {
      "envelope": "CloudEvents/1.0",
      "messages": {
        "myevent": {
          "envelope": "CloudEvents/1.0",
          "envelopemetadata": {
            "id": {
              "type": "string",
              "required": true
            },
            "type": {
              "type": "string",
              "value": "myevent",
              "required": true
            },
            "time": {
              "type": "datetime",
              "required": true
            },
            "source": {
              "type": "uritemplate",
              "value": "/me",
              "required": true
            }
          }
        }
      }
    }
  }
}
