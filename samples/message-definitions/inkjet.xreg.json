                    "type": "string"
                  },
                  {
                    "name": "deviceid",
                    "doc": "Device identifier",
                    "type": "string"
                  },
                  {
                    "name": "jamLocation",
                    "doc": "Location within the printer where the jam occurred",
                    "type": "string"
                  },
                  {
                    "name": "cleared",
                    "doc": "Whether the jam has been cleared (true/false)",
                    "type": "boolean"
                  }
                ]
              }
            }
          }
