                "fields": [
                  {
                    "name": "tenantid",
                    "doc": "Tenant identifier",
                    "type": "string"
                  },
                  {
                    "name": "deviceid",
                    "doc": "Device identifier",
                    "type": "string"
                  },
                  {
                    "name": "cartridgeId",
                    "doc": "Identifier of the low ink cartridge",
                    "type": "string"
                  },
                  {
                    "name": "inkColor",
                    "doc": "Color of the ink that is low",
                    "type": {
                      "type": "enum",
                      "name": "InkColor",
                      "symbols": [
                        "Black",
                        "Cyan",
                        "Magenta",
                        "Yellow"
                      ]
                    }
                  },
                  {
                    "name": "remainingLevel",
                    "doc": "Remaining ink level percentage",
                    "type": "int"
                  }
                ]
              }
            }
          }
        },
